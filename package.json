{
  "name": "stylelint-config-cloudfour-suit",
  "version": "6.1.0",
  "description": "Cloud Four's stylelint config + SUIT naming",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "cloudfour"
  ],
  "repository": "cloudfour/stylelint-config-cloudfour-suit",
  "license": "MIT",
  "author": "Cloud Four",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "run-s lint:js lint:md:check lint:prettier",
    "lint:check": "run-s lint:*:check",
    "lint:js": "eslint --fix .",
    "lint:js:check": "eslint .",
    "lint:md:check": "remark . --quiet --frail",
    "lint:prettier": "prettier --write .",
    "lint:prettier:check": "prettier --list-different .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prettier": "@stylelint/prettier-config",
  "eslintConfig": {
    "extends": [
      "stylelint"
    ],
    "globals": {
      "module": true,
      "require": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "dependencies": {
    "stylelint-config-cloudfour": "^6.1.0",
    "stylelint-selector-bem-pattern": "^2.0.0"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "2.0.0",
    "@stylelint/remark-preset": "3.0.0",
    "eslint": "8.23.0",
    "eslint-config-stylelint": "15.1.0",
    "jest": "29.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "remark-cli": "11.0.0",
    "stylelint": "14.11.0"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  }
}
